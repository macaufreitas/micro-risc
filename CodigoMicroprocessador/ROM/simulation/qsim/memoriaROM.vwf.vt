`timescale 1 ps/ 1 ps
module memoriaROM_vlg_sample_tst(
	chipenable,
	clk,
	endereco,
	sampler_tx
);
input  chipenable;
input  clk;
input [7:0] endereco;
output sampler_tx;

reg sample;
time current_time;
always @(chipenable or clk or endereco)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

