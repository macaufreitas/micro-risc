	endereco[3] = #520000 1'b0;
end 
// endereco[ 2 ]
initial
begin
	endereco[2] = 1'b1;
	endereco[2] = #520000 1'b0;
	endereco[2] = #160000 1'b1;
	endereco[2] = #160000 1'b0;
end 
// endereco[ 1 ]
initial
begin
	endereco[1] = 1'b1;
	endereco[1] = #360000 1'b0;
