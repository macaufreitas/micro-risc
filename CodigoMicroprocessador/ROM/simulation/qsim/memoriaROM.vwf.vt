module memoriaROM_vlg_vec_tst();
// constants                                           
// general purpose registers
reg chipenable;
reg clk;
reg [7:0] endereco;
// wires                                               
wire [15:0] saida;

wire sampler;                             

// assign statements (if any)                          
memoriaROM i1 (
// port map - connection between master ports and signals/registers   
	.chipenable(chipenable),
	.clk(clk),
	.endereco(endereco),
	.saida(saida)
);

// chipenable
initial
begin
	chipenable = 1'b1;
end 

// clk
initial
begin
	repeat(6)
	begin
		clk = 1'b0;
		clk = #80000 1'b1;
		# 80000;
	end
	clk = 1'b0;
end 
// endereco[ 7 ]
initial
begin
	endereco[7] = 1'b1;
	endereco[7] = #360000 1'b0;
end 
// endereco[ 6 ]
initial
begin
	endereco[6] = 1'b1;
	endereco[6] = #360000 1'b0;
end 
// endereco[ 5 ]
